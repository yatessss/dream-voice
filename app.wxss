/*
 * 1px
 * ui-px：上下边线
 * ui-px-a：下边线
 * ui-px-b：上边线
 * ui-px-r：右边线
 * ui-dashed-a：下虚线
 */

.ui-px, .ui-px-a, .ui-px-b, .ui-px-r, .ui-px-l, .ui-dashed-a{
  position: relative;
}
.ui-px::before,
.ui-px::after {
  position: absolute;width: 100%;content: '';left: 0;
}
.ui-px::before,
.ui-px-b::before {
  top: 0;border-top:1px solid #e5e5e5;transform-origin:100% 0;
}
.ui-px::after,
.ui-px-a::after,
.ui-dashed-a::after {
  bottom: 0;border-bottom:1px solid #e5e5e5;transform-origin:0 100%;
}
.ui-dashed-a::after {border-bottom-style: dashed;}
.ui-px-a::after,
.ui-px-b::before,
.ui-dashed-a::after {
  position: absolute;width: 100%;content: '';left: 0;
}
.ui-px:before,
.ui-px:after,
.ui-px-a:after,
.ui-px-b:before,
.ui-dashed-a::after {
  transform:scaleY(.5);
}
.ui-px-r::after, .ui-px-l::before {
  height:100%;content:'';border-right: 1px solid #e5e5e5;
  position:absolute;top:0;transform:scaleX(.5);
}
.ui-px-r::after{right: 0;}
.ui-px-l::before{left: 0;}

/* 盒形边框，带圆角 */
.ui-box-px {
  box-sizing: border-box;
  overflow: hidden;
  position: relative;
}
.ui-box-px::before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  border: 1px solid #e0e0e0;
  padding: 1px;
  box-sizing: border-box;
  transform-origin: 0 0!important;
  pointer-events: none;
}
@media (-webkit-min-device-pixel-ratio:1.5), (min-device-pixel-ratio:1.5){
.ui-box-px::before{border-radius:6px;width:149.9%;height:149.9%;transform:scale(.6666);}
}
@media (-webkit-min-device-pixel-ratio:2), (min-device-pixel-ratio:2){
.ui-box-px::before{border-radius:8px;width:199.9%;height:199.9%;transform:scale(.5);}
}
@media (-webkit-min-device-pixel-ratio:3), (min-device-pixel-ratio:3){
.ui-box-px::before{border-radius:12px;width:299.9%;height:299.9%;transform:scale(.3333);}
}


