.hj-by-wrap .ui-px-r {
  border-right-color: #fff;
}

/*
 * 垂直时间戳
 */
.mod-v-timestamp{display: inline-block;font-size: 13px;margin-left: 6px;color: #888f9b;line-height: 1;}
.mod-v-timestamp .node::before{width: 2px;background-color: #d4d4d4;content: "";left: -2px;position:absolute;top:5px;height: 100%;z-index:-2;}
.mod-v-timestamp .node{position: relative;margin-bottom: 33px;}
.mod-v-timestamp .node .desc{padding-left: 105px;padding-bottom: 3px;}
.mod-v-timestamp .node .header {position: absolute;top: 0;left: -7px;width: 110px;display: -webkit-box;display: flex;}
.mod-v-timestamp .node .dot{display: inline-block;width: 6px;height: 6px;border-radius: 6px;border: 3px solid #d4d4d4;background-color: #d4d4d4;margin-right: 10px;position: relative;}
.mod-v-timestamp .node .dot::before{position: absolute;content: '';width: 2px;height: 35px;background-color: #d4d4d4;bottom:3px;left: 2px;z-index: -1;}
.mod-v-timestamp .on{color: #f29360;}
.mod-v-timestamp .on::before{background-color: #f29360;}
.mod-v-timestamp .on .dot{border: 3px solid #ffd9c5;background-color: #f39361;}
.mod-v-timestamp .on .dot::before{background-color: #f39361;}
.mod-v-timestamp .first .dot::before{content: none;}
.mod-v-timestamp .last{margin-bottom: 5px;}
.mod-v-timestamp .last::before{display: none;}

/* 旧版时间戳 */

.date-list-wrap{margin: 30px 0 20px;text-align: left;}
.date-list {display: inline-block;font-size: 28rpx;}
.date-list .li {color: #d4d4d4;line-height: 16px;overflow: hidden;}
.date-list .li .i{float: left;width: 16px;height: 16px;background-color: currentColor;border-radius: 16px;margin-right: 10px;}
.date-list .li .t {color: #445c95;}
.date-list .o, .date-list .o .t {color: #f29360;}
.date-list .sx .i{float: left;height: 40px;width: 6px;margin-left: 7px;border-radius: 0;border-left: 2px solid currentColor;background: none;}
.date-list .do {line-height: 50px;color: #acb2c1;}
.date-list .do .i{border-left: 2px dotted currentColor;height: 50px;}


/*
 * 微众卡
*/
.we-card {
  background: #445C95;
  border-radius:12rpx;
  box-sizing:border-box;
  font-size:24rpx;
  color:#98a5c4;
  overflow: hidden;
  padding:24rpx 36rpx;
  position: relative;
  z-index: 1;
}
.we-card .cbg {
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  z-index: -1;
}
.we-card .cbg::after {
  content: '';
  display: block;
  padding-top: 37.67%;
}
.we-card .h1 {
  margin-bottom: 22rpx;
}
.we-card .bg-card {
  width: 100%;
  height: 100%;
  position: absolute;
}
.we-card .num{
  font-size:28rpx;
}
.we-card .copy {
  color: #fff;
  font-size: 26rpx;
  text-decoration: underline;
}
/* 长而扁版本 */
.we-card-v2 {
  border-radius: 16rpx;
  font-size: 28rpx;
  line-height: 1;
  padding: 30rpx 48rpx 24rpx;
}
.we-card-v2 .cbg::after {
  padding-top: 22.07%;
}
.we-card-v2 .h1 {
  margin-bottom: 19rpx;
}
.we-card-v2 .bg-card {
  top: 30%;
